{
    "name": "ros-snippets",
    "displayName": "ROS Snippets",
    "description": "ROS (Robot Operating System) snippets for rospy, roscpp, launch, urdf, xacro etc.",
    "version": "0.1.0",
    "publisher": "sweilz",
    "icon": "media/icon.png",
    "keywords": [
        "ros",
        "robot"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/SweiLz/ROS-Snippets"
    },
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "xml",
                "extensions": [
                    "launch",
                    "srdf",
                    "urdf",
                    "xacro"
                ]
            },
            {
                "id": "yaml",
                "extensions": [
                    "rviz"
                ]
            }
        ],
        "snippets": [
            {
                "language": "xml",
                "path": "./snippets/ros_urdf_snippets.json"
            },
            {
                "language": "python",
                "path": "./snippets/ros_py_snippets.json"
            },
            {
                "language": "cpp",
                "path": "./snippets/ros_cpp_snippets.json"
            }
        ]
    }
}